{
    "typescript.validate.enable": true,
    "search.exclude": {
        "*.json": true,
        "**/dist": true,
        "**/excercise_data_set": true,
        "**/migrations": true,
        // "**/node_modules**": true,
    },
    "files.exclude": {
        "**/dist": true,
        "**/excercise_data_set": true,
        "**/cleaned_data_set": true,
        "**/muscle_data_set": true,
